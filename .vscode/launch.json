{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug - Notebook format",
      "cargo": {
        "args": ["build", "--bin=main", "--package=semTParser"],
        "filter": {
          "name": "main",
          "kind": "bin"
        }
      },
      "args": ["-l", "./logs.txt", "-t", "./table_1.csv", "-f", "notebook"],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "sourceLanguages": ["rust"],
      "expressions": "native"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug - Python format",
      "cargo": {
        "args": ["build", "--bin=main", "--package=semTParser"],
        "filter": {
          "name": "main",
          "kind": "bin"
        }
      },
      "args": ["-l", "./logs.txt", "-t", "./table_1.csv", "-f", "python"],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "sourceLanguages": ["rust"],
      "expressions": "native"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'main'",
      "program": "${workspaceFolder}/target/debug/main",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "preLaunchTask": "cargo build",
      "sourceLanguages": ["rust"],
      "expressions": "native"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug - Old logs Python",
      "cargo": {
        "args": ["build", "--bin=main", "--package=semTParser"],
        "filter": {
          "name": "main",
          "kind": "bin"
        }
      },
      "args": ["-l", "./logs_old.txt", "-t", "./table_1.csv", "-f", "python"],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "sourceLanguages": ["rust"],
      "expressions": "native"
    }
  ]
}
