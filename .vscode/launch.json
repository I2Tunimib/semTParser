{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'main'",
      "cargo": {
        "args": ["build", "--bin=main", "--package=semTParser"],
        "filter": {
          "name": "main",
          "kind": "bin"
        }
      },
      "program": "${workspaceFolder}/target/debug/main",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false
    }
  ]
}
